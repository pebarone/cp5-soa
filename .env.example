# Configuração do Banco de Dados Oracle
# Substitua com suas credenciais reais
DB_USER=seu_usuario
DB_PASSWORD=sua_senha
DB_URL=localhost:1521/ORCL

# Porta da aplicação (opcional)
SERVER_PORT=3000

# Ambiente Node.js (opcional)
NODE_ENV=production

# Seed automático ao iniciar o container (opcional)
# true = popula banco com dados de exemplo (10 hóspedes, 10 quartos, 10 reservas)
# false = não executa seed
AUTO_SEED=false
